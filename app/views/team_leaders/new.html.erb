<div class="well">
<h1>New Team Leader</h1>
</div>
<div class="well">
<%= simple_form_for(@team_leader) do |f| %>
	<%= f.simple_fields_for :user do |u|%>
		<%= u.input :email, :placeholder => "Email"%>
		<%= u.input :password, :placeholder => "Password" %>
		<%= u.input :password_confirmation, :placeholder => "Password confirmation" %>
	<% end %>	
	<%= f.submit "Submit", :class => "btn btn-primary"%>
</div>	
	<% end %>
<div class= "well">	
<%= link_to 'Back', team_leaders_path, :class => "btn btn-primary" %>
</div>